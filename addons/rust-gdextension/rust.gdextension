[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.3
reloadable = true

[libraries]
linux.debug.x86_64 =     "target/debug/librust_gdextension.so"
linux.release.x86_64 =   "target/release/librust_gdextension.so"
windows.debug.x86_64 =   "target/debug/rust_gdextension.dll"
windows.release.x86_64 = "target/release/rust_gdextension.dll"
macos.debug =            "target/debug/librust_gdextension.dylib"
macos.release =          "target/release/librust_gdextension.dylib"
macos.debug.arm64 =      "target/debug/librust_gdextension.dylib"
macos.release.arm64 =    "target/release/librust_gdextension.dylib"

[icons]
Player = "icon.png"
